<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_locatr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.enerjizeit.locatr.LocatrActivity">

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/image"
        android:background="@android:color/white"
        app:layout_constraintBottom_toBottomOf="@+id/activity_locatr"
        app:layout_constraintLeft_toLeftOf="@+id/activity_locatr"
        app:layout_constraintRight_toRightOf="@+id/activity_locatr"
        app:layout_constraintTop_toTopOf="@+id/activity_locatr" />

</android.support.constraint.ConstraintLayout>
